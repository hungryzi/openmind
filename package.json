{
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "rimraf": "^2.6.2",
    "truffle": "^4.1.7"
  },
  "dependencies": {
    "node-fetch": "^2.1.2"
  },
  "scripts": {
    "build": "rimraf dist/ && babel ./scraper --out-dir dist/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log --copy-files",
